  "scripts": {
    "start": "slate-tools start",
    "watch": "slate-tools start --skipFirstDeploy",
    "build": "slate-tools build",
    "deploy": "slate-tools build && slate-tools deploy",
    "zip": "slate-tools build && slate-tools zip",
    "audit-dump": "list-selectors src/assets/pattern-lab-style-audit.css > src/assets/pattern-lab-style-audit.json -pretty",
    "audit-dump-test": "list-selectors node_modules/tachyons/css/tachyons.css > src/assets/tachyons-style-audit.json -pretty",
    "style-dump": "node-sass  src/assets/pattern-lab-style-audit.scss src/assets/pattern-lab-style-audit.css",
    "audit": "npm run style-dump && npm run audit-dump ",
    "styles": "node-sass  -r --watch -rw  --include-path node_modules src/assets/pattern-lab-style-audit.scss src/assets/pattern-lab-style-audit.css",
    "get-patternlab": "git clone https://github.com/gbtunney/g-Patternlab.git src/assets/g-Patternlab"
